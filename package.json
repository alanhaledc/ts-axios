{
  "name": "ts-axios",
  "version": "0.2.0",
  "description": "",
  "keywords": [],
  "main": "dist/axios.umd.js",
  "module": "dist/axios.es5.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Hale Deng <haledeng88@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/haledc/ts-axios"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "dev": "node examples/server.js",
    "lint": "prettier --write --parser typescript \"src/**/*.ts?(x)\" \"test/**/*.ts?(x)\"",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts && typedoc --out docs --target es6 --theme minimal --mode file src",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "precommit": "lint-staged",
    "travis-deploy-once": "travis-deploy-once",
    "prepub": "yarn test:prod && yarn build",
    "pub": "sh release.sh"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-typescript": "^4.1.1",
    "@types/follow-redirects": "^1.8.0",
    "@types/jasmine-ajax": "^3.3.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.0",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "commitizen": "^4.0.4",
    "connect-multiparty": "^2.2.0",
    "cookie-parser": "^1.4.5",
    "coveralls": "^3.0.11",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.1",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "express": "^4.17.1",
    "husky": "^4.2.3",
    "jasmine-ajax": "^4.0.0",
    "jasmine-core": "^3.5.0",
    "jest": "^25.2.7",
    "jest-config": "^25.2.7",
    "lint-staged": "^10.1.2",
    "lodash.camelcase": "^4.3.0",
    "nprogress": "^0.2.0",
    "prettier": "^2.0.4",
    "prompt": "^1.0.0",
    "qs": "^6.9.3",
    "replace-in-file": "^5.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.3.4",
    "rollup-plugin-sourcemaps": "^0.5.0",
    "semantic-release": "^17.0.4",
    "shelljs": "^0.8.3",
    "style-loader": "^1.1.3",
    "travis-deploy-once": "^5.0.11",
    "ts-jest": "^25.5.0",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.2",
    "tslib": "^1.11.2",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  },
  "dependencies": {
    "follow-redirects": "^1.11.0"
  }
}
